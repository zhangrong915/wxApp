<view class="page">
    <view class="td-login-wrap td-content">
        <input class="td-input" type="text" value="{{username}}" bindinput="getUsername" placeholder="请输入登录账号"/>
        <input class="td-input" type="password" value="{{password}}" bindinput="getPassword" placeholder="请输入登录密码" />
        <button class="td-btn"  bindtap="handleLogin" >登录</button>
    </view>
</view>